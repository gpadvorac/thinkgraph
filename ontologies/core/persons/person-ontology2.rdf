<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:person="http://example.org/person#">

  <!-- Definition of the Person Class -->
  <rdfs:Class rdf:about="http://example.org/person#Person">
    <rdfs:label>Person</rdfs:label>
    <rdfs:comment>A class representing a person.</rdfs:comment>
  </rdfs:Class>

  <!-- Definition of the DID property -->
  <rdf:Property rdf:about="http://example.org/person#DID">
    <rdfs:label>DID</rdfs:label>
    <rdfs:comment>Decentralized ID of the person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/person#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <!-- Definition of the givenName property -->
  <rdf:Property rdf:about="http://example.org/person#givenName">
    <rdfs:label>givenName</rdfs:label>
    <rdfs:comment>Given name of the person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/person#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <!-- Definition of the middleName property -->
  <rdf:Property rdf:about="http://example.org/person#middleName">
    <rdfs:label>middleName</rdfs:label>
    <rdfs:comment>Middle name or initial of the person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/person#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <!-- Definition of the familyName property -->
  <rdf:Property rdf:about="http://example.org/person#familyName">
    <rdfs:label>familyName</rdfs:label>
    <rdfs:comment>Family name of the person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/person#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <!-- Definition of the birthDate property -->
  <rdf:Property rdf:about="http://example.org/person#birthDate">
    <rdfs:label>birthDate</rdfs:label>
    <rdfs:comment>Year and day of birth of the person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/person#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
  </rdf:Property>

  <!-- Definition of the timeStamp property -->
  <rdf:Property rdf:about="http://example.org/person#timeStamp">
    <rdfs:label>timeStamp</rdfs:label>
    <rdfs:comment>Date and time this person data was created.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/person#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdf:Property>

</rdf:RDF>
